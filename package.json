{
  "name": "@blendsdk/express",
  "version": "0.9.1",
  "description": "Common library and utilities to use Git in NodeJS",
  "main": "dist/index.js",
  "author": "Gevik Babakhani <gevikb@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/blendsdk/express.git"
  },
  "keywords": [
    "git",
    "utility"
  ],
  "bin": {
    "is_git_branch" : "./dist/utils/is_git_branch.js"
  },
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -fR ./dist && tsc",
    "watch": "rm -fR ./dist && tsc -w ",
    "fiddle": "node ./dist/fiddle.js",
    "test": "jest --detectOpenHandles",
    "patch-publish": "node ./dist/is_git_branch master && yarn build && yarn publish --patch --access public && git push origin master --tags"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "shelljs": "^0.8.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/shelljs": "^0.8.5",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  }
}
